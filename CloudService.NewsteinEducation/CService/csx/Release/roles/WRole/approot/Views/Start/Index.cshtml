    <!-- ******CONTACT****** -->
        <div id="contact" class="container">
        <br/><br /><br /><br />
            <div class="row text-center">
                <h2 class="title">Contact US</h2>
                <div class="intro col-md-6 col-sm-12 col-xs-12 col-md-offset-3">
                    <p>We’d love to hear from you. Please feel free to comment and provide us with your insight in project. Join us in this revolutionary education era.</p>
                    <ul class="list-unstyled contact-details">
                        <li><i class="fa fa-envelope"></i><a target="_blank" href="mailto:contact@newsteineducation.com.lily.arvixe.com">contact@newsteineducation.com</a></li>
                        <li><i class="fa fa-phone-square"></i>(619) 245-5130</li>
                    </ul>
                </div>
            </div><!--//row-->
            <div class="row text-center">
                <div class="contact-form col-md-6 col-sm-12 col-xs-12 col-md-offset-3">

                    @using (Html.BeginForm("Index", "Start", FormMethod.Post, new { @class = "form", role = "form" }))
                    {
                        @Html.AntiForgeryToken()

                        
                          <div class="form-group name">
                            <label class="sr-only" for="name">Name</label>

                              @Html.TextBoxFor(m => m.Name, new { @type = "text", @placeholder = "Name:", @class = "form-control" })
                              @Html.ValidationMessageFor(m => m.Name)


                        </div><!--//form-group-->

                        <div class="form-group email">
                            <label class="sr-only" for="email">Email</label>
                            @Html.TextBoxFor(m => m.Email, new { @type = "email", @placeholder = "Email:", @class = "form-control" })
                            @Html.ValidationMessageFor(m => m.Email)




                        </div><!--//form-group-->

                        <div class="form-group message">
                            <label class="sr-only" for="message">Message</label>

                            @Html.TextAreaFor(m => m.Message, new { @rows = "6", @placeholder = "Message:", @class = "form-control" })
                            @Html.ValidationMessageFor(m => m.Message)
   </div><!--//form-group-->

                        
             
                        <button type="submit" class="btn btn-lg btn-theme">Send Message</button>
                        

                  
                    }



                      
                   <!--//form-->
                </div><!--//contact-form-->
            </div><!--//row-->
            <div class="text-center">
                <ul class="social-icons list-inline">
                    <li><a target="_blank" href="https://twitter.com/newsteinsystems"><i class="fa fa-twitter"></i></a></li>
